{
    "version": 2,
    "builds": [
      {
        "src": "angular.json",
        "use": "@vercel/angular"
      },
      {
        "src": "db.json",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/api/db.json"
      },
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }
  